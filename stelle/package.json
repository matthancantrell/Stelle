{
  "name": "stelle",
  "displayName": "Stelle",
  "description": "",
  "version": "0.0.1",
  "publisher": "Matthan Cantrell",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "stelle.rightClickCommand",
          "when": "editorHasSelection",
          "group": "extension"
        }
      ]
    },
    "commands": [
      {
        "command": "stelle.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "stelle.testCommand",
        "title": "Test Command"
      },
      {
        "command": "stelle.start",
        "title": "Start coding with Stelle!"
      },
      {
        "command": "stelle.rightClickCommand",
        "title": "Stelle's First Context Menu"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "openai": "^4.0.0",
    "@vscode/test-electron": "^2.3.4"
  }
}
