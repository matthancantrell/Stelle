{
  "name": "stelle",
  "displayName": "Stelle",
  "description": "An AI Powered Asisstant To Help You Program!",
  "keywords": ["Stelle", "AI Asisstant", "Programming", "Teacher"],
  "version": "1.0.0",
  "publisher": "MatthanCantrell",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "stelle.stelleGroup",
        "label": "Stelle"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "stelle.stelleGroup",
          "label": "Stelle"
        }
      ],
      "stelle.stelleGroup": [
        {
          "command": "stelle.analyze"
        },
        {
          "command": "stelle.optimize"
        },
        {
          "command": "stelle.fix"
        }
      ]
    },
    "commands": [
      {
        "command": "stelle.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "stelle.analyze",
        "title": "Analyze This Code"
      },
      {
        "command": "stelle.optimize",
        "title": "Optimize This Code"
      },
      {
        "command": "stelle.fix",
        "title": "Fix This Code"
      },
      {
        "command": "stelle.testCommand",
        "title": "Test Command"
      },
      {
        "command": "stelle.start",
        "title": "Start coding with Stelle!"
      },
      {
        "command": "stelle.rightClickCommand",
        "title": "Stelle's First Context Menu"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "openai": "^4.14.2",
    "typescript": "^5.2.2"
  }
}
